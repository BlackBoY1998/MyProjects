
@{
    ViewBag.Title = "test";
}

<html>
<head>

    <title></title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.5.0/Chart.min.js"></script>
</head>
<body>

    <canvas id="myChart" style="width:100%;max-width:600px"></canvas>

    <script>
       
        var xValues = ["11:15-11:20", "11:20-11:25", "11:25-11:30", "11:30-11:35", "11:40-11:45"];
        var yValues = [55,34,11,13,35];
        var barColors = ["red", "green", "blue", "orange", "brown"];

        new Chart("myChart", {
            type: "bar",
            data: {
                labels: xValues,
                datasets: [{
                    backgroundColor: barColors,
                    data: yValues
                }]
            },
            options: {
                legend: { display: false },
                title: {
                    display: true,
                    text: "Talk Time of 15 Min"
                }
            }
           
        });
        
    </script>

</body>
</html>

